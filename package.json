{
	"name": "vscode-charts",
	"displayName": "vscode-charts",
	"description": "vscode-charts",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.39.0"
	},
	"categories": [
		"Programming Languages"
	],
	"activationEvents": [
		"onCommand:chart.view"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "chart.view",
				"title": "Chart View"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.11.1",
		"@types/vscode": "^1.39.0",
		"glob": "^7.1.4",
		"mocha": "^6.2.2",
		"typescript": "^3.6.4",
		"tslint": "^5.20.0",
		"vscode-test": "^1.2.0"
	}
}
